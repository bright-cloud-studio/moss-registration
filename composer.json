{
    "name":"bright-cloud-studio/moss-registration",
    "description":"Customizes the registration fields for Moss Nutrition",
    "keywords": ["moss", "registration", "bright", "cloud", "studio"],
    "type": "contao-bundle",
    "homepage": "http://brightcloudstudio.com/",
    "license": "LGPL-3.0+",
    "authors":[
        {
            "name": "Bright Cloud Studio",
            "homepage": "https://brightcloudstudio.com",
            "email": "mark@brightcloudstudio.com",
            "role": "Developer"
        }
    ],
    "support":{
        "issues": "https://github.com/bright-cloud-studio/moss-registration/issues",
        "source": "https://github.com/bright-cloud-studio/moss-registration"
    },
    "require":{
        "php": "^8.1",
        "contao/core-bundle": "^4.13",
        "terminal42/contao-fineuploader": "^3.3",
    },
    "extra":{
        "contao-manager-plugin": "Bcs\\MossRegistrationBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\MossRegistrationBundle\\": "src"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    }
}
